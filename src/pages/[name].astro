---
import WelcomeLayout from "@/layouts/WelcomeLayout.astro"
import WelcomeSection from "@/components/booking/welcomeSection"

const commerceName = Astro.params.name

if (!commerceName) {
  return Astro.redirect("/404")
}
---
<WelcomeLayout>
  <WelcomeSection commerceName={commerceName} client:load/>
</WelcomeLayout>

