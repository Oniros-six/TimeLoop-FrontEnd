{
  "builds": [
    {
      "src": "landingPage/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "adminPanel/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/adminPanel/dist/index.html"
    },
    {
      "source": "/about",
      "destination": "/adminPanel/index.html"
    },
    {
      "source": "/a",
      "destination": "/adminPanel/"
    },
    {
      "source": "/b",
      "destination": "/adminPanel/dist/"
    }
  ]
}