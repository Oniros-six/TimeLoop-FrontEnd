{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    {
      "src": "/landingPage/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "/adminPanel/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/adminPanel/dist/index.html"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/adminPanel/dist/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/landingPage/dist/index.html"
    }
  ]
}