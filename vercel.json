{
  "version": 2,
  "builds": [
    {
      "src": "landingPage/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "adminPanel/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/admin(/.*)?", "dest": "/adminPanel/dist/index.html" }
  ]
}