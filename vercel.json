{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)\\.(css|js|mjs|png|svg|ico|jpg|jpeg|gif|webp|avif|woff2)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/favicon.ico",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/admin", "destination": "/adminPanel/dist/index.html" },
    { "source": "/admin/(.*)", "destination": "/adminPanel/dist/index.html" },
    { "source": "/(.*)", "destination": "/landingPage/dist/index.html" }
  ]
}